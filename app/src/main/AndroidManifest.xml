<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.cektrend.trashget">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Trashget"
        tools:ignore="ExtraText">
        <activity android:name=".admin.EditTrashActivity"></activity>
        <activity android:name=".customer.AlertActivity" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <activity
            android:name=".collector.TrackCollector"
            android:label="@string/title_activity_track_collector" />
        <activity
            android:name=".admin.AdminMapsActivity"
            android:label="@string/title_activity_admin_maps" />
        <activity android:name=".admin.DetailTrashActivity" />
        <activity android:name=".admin.ListTrashActivity" />
        <activity android:name=".customer.PeopleActivity" />
        <activity android:name=".customer.PeopleLogin" />
        <activity android:name=".customer.GetLocation" />
        <activity android:name=".admin.AddDriver" />
        <activity android:name=".collector.TrackTruck" />
        <activity android:name=".collector.TrackTrash" />
        <activity android:name=".admin.AdminActivity" />
        <activity android:name=".admin.AdminLogin" />
        <activity android:name=".customer.GetRequest" />
        <activity android:name=".collector.CollectorActivity" />
        <activity android:name=".collector.CollectorLogin" />
        <activity android:name=".MainActivity" />
        <activity
            android:name=".SplashActivity"
            android:noHistory="true"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service android:name=".customer.NotificationService" />

        <meta-data
            android:name="OnlineMaps.Key"
            android:value="z9WzsO2W3pFnvNyPGrQnPDYI0m1Q24Lr" />
        <meta-data
            android:name="OnlineRouting.Key"
            android:value="z9WzsO2W3pFnvNyPGrQnPDYI0m1Q24Lr" />
        <meta-data
            android:name="OnlineSearch.Key"
            android:value="z9WzsO2W3pFnvNyPGrQnPDYI0m1Q24Lr" />
        <meta-data
            android:name="OnlineTraffic.Key"
            android:value="z9WzsO2W3pFnvNyPGrQnPDYI0m1Q24Lr" />
        <meta-data
            android:name="GeofencingApi.Key"
            android:value="z9WzsO2W3pFnvNyPGrQnPDYI0m1Q24Lr" />
    </application>

</manifest>